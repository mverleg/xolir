{
  "name": "xolir",
  "version": "0.1.0",
  "description": "TypeScript protobuf classes for Tel IR api",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run generate-proto && tsc",
    "generate-proto": "mkdir -p target && npx pbjs -t static-module -w commonjs -p ../proto -o target/xolir.js ../proto/xolir/*.proto && npx pbts -o target/xolir.d.ts target/xolir.js",
    "clean": "rm -rf dist/ target/",
    "test": "node test.js"
  },
  "keywords": [
    "protobuf",
    "typescript",
    "xolir"
  ],
  "author": "Mark Verleg <mverleg.noreply@gmail.com>",
  "license": "CC-BY-NC-ND-4.0",
  "homepage": "https://github.com/mverleg/xolir",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mverleg/xolir.git"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "protobufjs": "^7.2.0",
    "protobufjs-cli": "^1.1.0",
    "@types/node": "^20.0.0"
  },
  "dependencies": {
    "protobufjs": "^7.2.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE.txt"
  ]
}
